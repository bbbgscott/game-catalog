<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.theragequit.game.catalog"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="16"
        android:targetSdkVersion="16" />

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@android:style/Theme.Holo" >
        <activity
            android:name="com.theragequit.game.catalog.Game_Catalog_Activity"
            android:label="@string/title_activity_game__catalog_" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.theragequit.game.catalog.VerifyActivity"
            android:label="@string/title_activity_verify" >
        </activity>
        <activity
            android:name="com.theragequit.game.catalog.ListActivity"
            android:label="@string/title_activity_list" >
        </activity>
        <activity
            android:name="com.theragequit.game.catalog.UpdateActivity"
            android:label="@string/title_activity_update" >
        </activity>
    </application>

</manifest>